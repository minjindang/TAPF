<%@ page language="java" contentType="text/html;charset=Big5" %>
<script language="Javascript">
function doBodyOnLoad(document)
{
	clearForm(document.forms[0]);
}
function changeYear(form)
{
	if(100 > Number(form.qryaccountYear.value) && 0 != Number(form.qryaccountYear.value))
	{
		if(3 > form.qryaccountYear.value.length)
		{
			form.qryaccountYear.value = "0" + form.qryaccountYear.value;
		}	
	}
}
</script>