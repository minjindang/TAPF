<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql.debt">
	<class
		name="AheadpayMain"
		table="aheadpay_main"
	>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="issueId"
			column="issue_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="paymentId"
			column="payment_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="fileNo"
			column="file_no"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="issueSerial"
			column="issue_serial"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="bondType"
			column="bond_type"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="accountYear"
			column="account_year"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="issueDate"
			column="issue_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="issueAmount"
			column="issue_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="bidAmount"
			column="bid_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="bidRate"
			column="bid_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="gotbidAmount"
			column="gotbid_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="orgissueAmount"
			column="orgissue_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="interestAmount"
			column="interest_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="lowRate"
			column="low_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="hightRate"
			column="hight_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="averageRate"
			column="average_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="dueDate"
			column="due_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="debtName"
			column="debt_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="issueDays"
			column="issue_days"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="aheadDate"
			column="ahead_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="manager"
			column="is_manager"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="createDate"
			column="create_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="modDate"
			column="mod_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="userId"
			column="userId"
			type="string"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>