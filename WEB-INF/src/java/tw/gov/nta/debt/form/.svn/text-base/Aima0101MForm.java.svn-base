	@SuppressWarnings("deprecation")
	public void setCashDueDate(Date cashDueDate) {
		if(getFinishDate() != null && getPayLimit() != null ){
			Calendar tCalendar = new GregorianCalendar();
			tCalendar.setTime(getFinishDate());
			tCalendar.add(Calendar.YEAR, getPayLimit());
			if(tCalendar.getTime().getYear()-11 <= 88 )
				tCalendar.set(tCalendar.get(Calendar.YEAR),5,30);
			else
				tCalendar.set(tCalendar.get(Calendar.YEAR),11,31);
			issueMain.setCashDueDate(tCalendar.getTime());
		}
	}
