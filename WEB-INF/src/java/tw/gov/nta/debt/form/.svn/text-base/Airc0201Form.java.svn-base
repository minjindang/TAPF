package tw.gov.nta.debt.form;

import java.util.Date;

import gov.dnt.tame.common.SupportForm;

public class Airc0201Form extends SupportForm {
	private static final long serialVersionUID = 1L;
		private Date limitDate ; //截止日期
		private Date issueSDate ; //發行日期 開始
		private Date issueEDate ; //發行日期 結束
		private String budgetCode ; //預算別
		
		public Date getLimitDate() {
			return limitDate;
		}
		public void setLimitDate(Date limitDate) {
			this.limitDate = limitDate;
		}
		public Date getIssueSDate() {
			return issueSDate;
		}
		public void setIssueSDate(Date issueSDate) {
			this.issueSDate = issueSDate;
		}
		public Date getIssueEDate() {
			return issueEDate;
		}
		public void setIssueEDate(Date issueEDate) {
			this.issueEDate = issueEDate;
		}
		public String getBudgetCode() {
			return budgetCode;
		}
		public void setBudgetCode(String budgetCode) {
			this.budgetCode = budgetCode;
		}
}
