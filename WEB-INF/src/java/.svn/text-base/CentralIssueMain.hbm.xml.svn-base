<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql.debt">
	<class
		name="CentralIssueMain"
		table="central_issue_main"
	>

		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>
		<property
			name="IssueId"
			column="issue_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="IssueSerial"
			column="issue_serial"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="DueDate"
			column="due_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="DebtName"
			column="debt_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="IssueDays"
			column="issue_days"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BondType"
			column="bond_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="IssueKind"
			column="issue_kind"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="AccountYear"
			column="account_year"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="IssueDate"
			column="issue_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="BidAmount"
			column="bid_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="BidRate"
			column="bid_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="ReachbidAmount"
			column="reachbid_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="GotbidAmount"
			column="gotbid_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="TotalbidAmount"
			column="totalbid_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="LowRate"
			column="low_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="HightRate"
			column="hight_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="AverageRate"
			column="average_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="SellAmount"
			column="sell_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="NouseAmount"
			column="nouse_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="Manager"
			column="is_manager"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="CreateDate"
			column="create_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="ModDate"
			column="mod_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Userid"
			column="userid"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="IssueAmount"
			column="issue_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>