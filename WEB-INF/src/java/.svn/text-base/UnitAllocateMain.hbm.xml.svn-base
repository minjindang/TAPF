<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql">
	<class
		name="UnitAllocateMain"
		table="Unit_allocate_main"
	>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="usrid"
			column="USRID"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="modDate"
			column="MOD_DATE"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="allMonth"
			column="ALL_MONTH"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="allMoney"
			column="ALL_MONEY"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="ok"
			column="IS_OK"
			type="yes_no"
			not-null="false"
			length="1"
		/>
		<property
			name="allYear"
			column="ALL_YEAR"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="monKind"
			column="MON_KIND"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="okDate"
			column="OK_DATE"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="okUser"
			column="OK_USER"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="srcMoney"
			column="SRC_MONEY"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="recvDate"
			column="RECV_DATE"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="sendDate"
			column="SEND_DATE"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="founda"
			column="FOUNDA"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="foundb"
			column="FOUNDB"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="lastFound"
			column="Last_Found"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="nowFound"
			column="Now_Found"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<many-to-one
			name="unitInfo"
			column="UNIT_INFO_ID"
			class="UnitInfoMain"
			not-null="true"
			lazy="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>