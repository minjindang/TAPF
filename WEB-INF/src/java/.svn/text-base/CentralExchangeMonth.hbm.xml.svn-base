<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql.debt">
	<class
		name="CentralExchangeMonth"
		table="central_exchange_month"
	>

		<property
			name="id"
			column="id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="usrid"
			column="usrid"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="modDate"
			column="mod_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="remark"
			column="remark"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="createDate"
			column="create_Date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="issueId"
			column="issue_id"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="bankId"
			column="BANK_ID"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="exgStock"
			column="exg_stock"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="exgAmount"
			column="exg_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="exgLessAmount"
			column="exg_less_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="totalLessAmount"
			column="total_less_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="termYm"
			column="TERM_YM"
			type="string"
			not-null="false"
			length="5"
		/>


	</class>	
</hibernate-mapping>