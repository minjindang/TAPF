<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql">
	<class
		name="CentralStatisticsMain"
		table="Central_statistics_main"
	>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="accountYear"
			column="ACCOUNT_YEAR"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="budgetCode"
			column="BUDGET_CODE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="debtCode"
			column="DEBT_CODE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="debtType"
			column="DEBT_TYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="boundCode"
			column="BOUND_CODE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="debt"
			column="IS_DEBT"
			type="yes_no"
			not-null="true"
			length="1"
		/>
		<property
			name="accountKind"
			column="ACCOUNT_KIND"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="account"
			column="IS_ACCOUNT"
			type="yes_no"
			not-null="true"
			length="1"
		/>
		<property
			name="issueAmount"
			column="ISSUE_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="payAmount"
			column="PAY_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="remainAmount"
			column="REMAIN_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="interestAmount"
			column="INTEREST_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="yearKind"
			column="year_kind"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="accountType"
			column="account_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="inoutType"
			column="inout_type"
			type="string"
			not-null="false"
			length="1"
		/>
	</class>	
</hibernate-mapping>