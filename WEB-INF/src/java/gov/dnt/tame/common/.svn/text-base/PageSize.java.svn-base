package gov.dnt.tame.common;

public class PageSize {
    public static final short LETTER_PAPERSIZE				=	1;
    public static final short LEGAL_PAPERSIZE				=	5;
    public static final short EXECUTIVE_PAPERSIZE			=	7;
    public static final short A3_PAPERSIZE					=	8;
    public static final short A4_PAPERSIZE					=	9;
    public static final short A5_PAPERSIZE					=	11;
    public static final short B4_PAPERSIZE				    =	12;
    public static final short ENVELOPE_10_PAPERSIZE			=	20;
    public static final short ENVELOPE_DL_PAPERSIZE			=	27;
    public static final short ENVELOPE_CS_PAPERSIZE			=	28;
    public static final short ENVELOPE_MONARCH_PAPERSIZE	=	37;
    
    public static short pageSize(String pageSize){
    	short page = 9;
    	if(pageSize.equals("LETTER_PAPERSIZE")) page =	1;
    	if(pageSize.equals("LEGAL_PAPERSIZE")) page =	5;
    	if(pageSize.equals("EXECUTIVE_PAPERSIZE")) page =	7;
    	if(pageSize.equals("A3_PAPERSIZE")) page =	8;
    	if(pageSize.equals("A4_PAPERSIZE")) page =	9;
    	if(pageSize.equals("A5_PAPERSIZE")) page =	11;
    	if(pageSize.equals("B4_PAPERSIZE")) page =	12;
    	if(pageSize.equals("ENVELOPE_10_PAPERSIZE")) page =	20;
    	if(pageSize.equals("ENVELOPE_DL_PAPERSIZE")) page =	27;
    	if(pageSize.equals("ENVELOPE_CS_PAPERSIZE")) page =	28;
    	if(pageSize.equals("ENVELOPE_MONARCH_PAPERSIZE")) page =	37;
		return page;
    }
}
