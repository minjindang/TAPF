<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql">
	<class
		name="TaxactionDet"
		table="Taxaction_det"
	>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="usrid"
			column="USRID"
			type="string"
			not-null="true"
			length="10"
		/>
		<property
			name="modDate"
			column="MOD_DATE"
			type="timestamp"
			not-null="true"
			length="23"
		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="true"
			length="23"
		/>
		<property
			name="nationTax"
			column="NATION_TAX"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="localTax"
			column="LOCAL_TAX"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="intTax"
			column="INT_TAX"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="taxKind"
			column="TAX_KIND"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="monNo"
			column="MON_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="bankno"
			column="BANKNO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="monKind"
			column="MON_KIND"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="pname"
			column="PNAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="taxYear"
			column="TAX_YEAR"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="taxMonth"
			column="TAX_MONTH"
			type="string"
			not-null="true"
			length="5"
		/>
		<property
			name="fillAmount"
			column="Fill_Amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="nationRate"
			column="NATION_RATE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="localRate"
			column="LOCAL_RATE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="intRate"
			column="INT_RATE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>