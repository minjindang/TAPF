<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql.debt">
	<class
		name="IssueExchangeDet"
		table="issue_exchange_det"
	>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="totalStockAmount"
			column="TOTAL_STOCK_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="getPrice"
			column="GET_PRICE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="backRate"
			column="BACK_RATE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="saleRate"
			column="SALE_RATE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="foreignCapital"
			column="FOREIGN_CAPITAL"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="exchangeStockCode"
			column="EXCHANGE_STOCK_CODE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="originalPrice"
			column="ORIGINAL_PRICE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="bankCode"
			column="BANK_CODE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="exchangeStockAmount"
			column="EXCHANGE_STOCK_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="liberateStockAmount"
			column="LIBERATE_STOCK_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="exchangeAmount"
			column="EXCHANGE_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="exchangeLessAmount"
			column="EXCHANGE_LESS_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="userid"
			column="USERID"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="modDate"
			column="MOD_DATE"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<many-to-one
			name="issue"
			column="ISSUE_ID"
			class="IssueMain"
			not-null="true"
			fetch="select"
			lazy="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>