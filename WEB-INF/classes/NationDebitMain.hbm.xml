<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql">
	<class
		name="NationDebitMain"
		table="Nation_debit_main"
	>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="accountYear"
			column="ACCOUNT_YEAR"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="gdpAmount"
			column="GDP_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="30"
		/>
		<property
			name="gnpAmount"
			column="GNP_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="30"
		/>
		<property
			name="pergnpAmount"
			column="PERGNP_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="30"
		/>
		<property
			name="budgetAmount"
			column="BUDGET_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="auditAmount"
			column="AUDIT_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="realAmount"
			column="REAL_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="keepAmount"
			column="KEEP_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="borrowAmount"
			column="BORROW_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="30"
		/>
		<property
			name="nopayAmount"
			column="NOPAY_AMOUNT"
			type="big_decimal"
			not-null="true"
			length="30"
		/>
		<property
			name="remainRate"
			column="REMAIN_RATE"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="gnpYearAmount"
			column="GNP_YEAR_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="gnpYearAmount1"
			column="GNP_YEAR_AMOUNT1"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="gnpAccountAmount"
			column="GNP_ACCOUNT_AMOUNT"
			type="big_decimal"
			not-null="false"
			length="30"
		/>
		<property
			name="caseRate"
			column="case_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="remainPayRate"
			column="remain_pay_rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<many-to-one
			name="nationCode"
			column="NATION_CODE"
			class="NationRef"
			not-null="true"
			fetch="select"
			lazy="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>