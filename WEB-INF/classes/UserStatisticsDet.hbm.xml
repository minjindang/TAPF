<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql">
	<class
		name="UserStatisticsDet"
		table="User_statistics_det"
	>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>
		<property
			name="accountYear"
			column="account_year"
			type="string"
			not-null="true"
			length="10"
		/>
		<property
			name="subAmount"
			column="sub_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="realAmount"
			column="real_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="keepAmount"
			column="keep_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="oldKeepAmount"
			column="old_keep_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="auditAmount"
			column="audit_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="plusdebtAmount"
			column="plusdebt_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="subdebtAmount"
			column="subdebt_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="dataType"
			column="data_type"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="banish"
			column="banish"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="userStatisticsMain"
			column="statistics_id"
			class="UserStatisticsMain"
			not-null="true"
			lazy="false"
		/>

	</class>	
</hibernate-mapping>