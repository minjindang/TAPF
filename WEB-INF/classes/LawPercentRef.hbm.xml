<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql">
	<class
		name="LawPercentRef"
		table="Law_percent_ref"
	>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="org.hibernate.id.IdentityGenerator"/>
		</id>

		<property
			name="usrid"
			column="USRID"
			type="string"
			not-null="true"
			length="10"
		/>
		<property
			name="modDate"
			column="MOD_DATE"
			type="timestamp"
			not-null="true"
			length="23"
		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="true"
			length="23"
		/>
		<property
			name="nationa"
			column="NATIONA"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="nationb"
			column="NATIONB"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="nationc"
			column="NATIONC"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="locala"
			column="LOCALA"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="localb"
			column="LOCALB"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="localc"
			column="LOCALC"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="inta"
			column="INTA"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="intb"
			column="INTB"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="intc"
			column="INTC"
			type="big_decimal"
			not-null="false"
			length="13"
		/>


	</class>	
</hibernate-mapping>