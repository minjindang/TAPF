<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="tw.gov.nta.sql.debt">
	<class
		name="CentralBondSepMonth"
		table="central_bond_sep_month"
	>

		<property
			name="id"
			column="id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="usrid"
			column="usrid"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="modDate"
			column="mod_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="remark"
			column="remark"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="createDate"
			column="create_Date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="issueId"
			column="issue_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="termMonth"
			column="TERM_MONTH"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="bankId"
			column="BANK_ID"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="sepAmount"
			column="sep_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="conAmount"
			column="con_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="nosepAmount"
			column="nosep_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="sepLessAmount"
			column="sep_less_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="totalAmount"
			column="total_amount"
			type="big_decimal"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>